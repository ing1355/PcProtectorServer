<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="oms.pc_protector.restApi.department.mapper.DepartmentMapper">

    <select id="selectDepartmentAll"
            resultType="oms.pc_protector.restApi.department.model.DepartmentVO" >
        SELECT * FROM department
    </select>

    <insert id="registerDepartmentByExcel" parameterType="oms.pc_protector.restApi.department.model.DepartmentVO">
        INSERT INTO department (name, code)
        VALUES (#{name}, #{code})
    </insert>

    <insert id="insertDepartment" parameterType="oms.pc_protector.restApi.department.model.DepartmentVO">
        INSERT INTO department (name, code)
        VALUES (#{name}, #{code})
    </insert>

    <delete id="deleteDepartmentAll">
        DELETE FROM department
    </delete>


</mapper>


